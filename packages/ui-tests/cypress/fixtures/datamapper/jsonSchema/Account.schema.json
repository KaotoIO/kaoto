  "properties": {
    "AccountId": {
      "type": "string"
    },
    "Name": {
      "type": "string"
    },
    "Address": {
      "type": "object",
      "properties": {
        "Street": {
          "type": "string"
        },
        "City": {
          "type": "string"
        },
        "State": {
          "type": "string"
        },
        "Country": {
          "type": "string"
        }
      },
      "required": [
        "Street",
        "City",
        "State",
        "Country"
      ]
