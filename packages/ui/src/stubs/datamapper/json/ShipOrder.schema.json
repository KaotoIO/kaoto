{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "OrderId": {
      "type": "string"
    },
    "OrderPerson": {
      "type": "string"
    },
    "ShipTo": {
      "type": "object",
      "properties": {
        "Name": {
          "type": "string"
        },
        "Street": {
          "type": "string"
        },
        "City": {
          "type": "string"
        },
        "State": {
          "type": "string"
        },
        "Country": {
          "type": "string"
        }
      },
      "required": [
        "Name",
        "Street",
        "City",
        "State",
        "Country"
      ]
    },
    "Item": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "Title": {
              "type": "string"
            },
            "Quantity": {
              "type": "integer"
            },
            "Price": {
              "type": "number"
            }
          },
          "required": [
            "Title",
            "Quantity",
            "Price"
          ]
        }
      ]
    }
  },
  "required": [
    "OrderId",
    "ShipTo"
  ]
}