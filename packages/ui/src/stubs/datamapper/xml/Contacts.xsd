<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="kaoto.datamapper.test">
    <xs:element name="Contacts">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Contact" maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="OrgName" type="xs:string" />
                            <xs:element name="PersonName" type="xs:string" />
                            <xs:element name="Email" type="xs:string" />
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>
